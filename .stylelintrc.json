{
  "extends": [
    "stylelint-config-tailwindcss"
  ],
  "rules": {
    "at-rule-no-unknown": [
      true,
      {
        "ignoreAtRules": [
          "apply",
          "utility", 
          "theme",
          "source",
          "custom-variant",
          "tailwind",
          "layer"
        ]
      }
    ]
  }
}
